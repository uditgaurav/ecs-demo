{
    "family": "paymentservice",
    "taskRoleArn": "arn:aws:iam::149554801296:role/ecsTaskExecutionRole",
    "executionRoleArn": "arn:aws:iam::149554801296:role/ecsTaskExecutionRole",
    "networkMode": "awsvpc",
    "containerDefinitions": [
      {
        "name": "paymentservice",
        "image": "gcr.io/google-samples/microservices-demo/paymentservice:v0.3.8",
        "cpu": 200,
        "memory": 128,
        "memoryReservation": 64,
        "portMappings": [
          {
            "containerPort": 50051,
            "hostPort": 50051,
            "protocol": "tcp"
          }
        ],
        "essential": true,
        "environment": [
          {
            "name": "PORT",
            "value": "50051"
          },
          {
            "name": "DISABLE_TRACING",
            "value": "1"
          },
          {
            "name": "DISABLE_PROFILER",
            "value": "1"
          },
          {
            "name": "DISABLE_DEBUGGER",
            "value": "1"
          }
        ],
        "healthCheck": {
          "retries": 3,
          "command": [
            "CMD-SHELL",
            "/bin/grpc_health_probe -addr=:50051"
          ],
          "timeout": 5,
          "interval": 5,
          "startPeriod": 60
        }
      }
    ],
    "requiresCompatibilities": [
      "FARGATE"
    ],
    "cpu": "256",
    "memory": "512"
  }
  