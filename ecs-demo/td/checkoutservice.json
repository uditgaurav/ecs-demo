{
    "family": "checkoutservice",
    "taskRoleArn": "arn:aws:iam::149554801296:role/ecsTaskExecutionRole",
    "executionRoleArn": "arn:aws:iam::149554801296:role/ecsTaskExecutionRole",
    "networkMode": "awsvpc",
    "containerDefinitions": [
      {
        "name": "checkoutservice",
        "image": "gcr.io/google-samples/microservices-demo/checkoutservice:v0.3.8",
        "cpu": 200,
        "memory": 128,
        "memoryReservation": 64,
        "portMappings": [
          {
            "containerPort": 5050,
            "hostPort": 5050,
            "protocol": "tcp"
          }
        ],
        "essential": true,
        "environment": [
          {
            "name": "PORT",
            "value": "5050"
          },
          {
            "name": "PRODUCT_CATALOG_SERVICE_ADDR",
            "value": "productcatalogservice:3550"
          },
          {
            "name": "SHIPPING_SERVICE_ADDR",
            "value": "shippingservice:50051"
          },
          {
            "name": "PAYMENT_SERVICE_ADDR",
            "value": "paymentservice:50051"
          },
          {
            "name": "EMAIL_SERVICE_ADDR",
            "value": "emailservice:5000"
          },
          {
            "name": "CURRENCY_SERVICE_ADDR",
            "value": "currencyservice:7000"
          },
          {
            "name": "CART_SERVICE_ADDR",
            "value": "cartservice:7070"
          },
          {
            "name": "DISABLE_STATS",
            "value": "1"
          },
          {
            "name": "DISABLE_TRACING",
            "value": "1"
          },
          {
            "name": "DISABLE_PROFILER",
            "value": "1"
          }
        ],
        "healthCheck": {
          "retries": 3,
          "command": [
            "CMD-SHELL",
            "/bin/grpc_health_probe -addr=:5050"
          ],
          "timeout": 5,
          "interval": 5,
          "startPeriod": 60
        }
      }
    ],
    "requiresCompatibilities": [
      "FARGATE"
    ],
    "cpu": "256",
    "memory": "512"
  }
  